

<% reviews.each(function(review) { %>
	<%=review.escape("name") %>'s dog <%=review.escape("name")%> -- 
	<%=review.escape("score")%>/5
	<% if(review.escape("comment")) { %>
	<%= review.escape("comment") %>
	<% } %>
	-- <%= moment(review.escape("created_at")).format("dddd, MMMM Do YYYY, h:mm:ss a") %> <br>
<% }) %>