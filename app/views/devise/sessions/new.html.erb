<div id="center-col">
<div id="signuptitle">Four Legs Good</div><br>

<div id="newuserbox">
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name, id: "newusersignup")) do |f| %>
  <%= f.email_field :email, :autofocus => true,  placeholder: "Email"  %><br><br>

  <%= f.password_field :password, placeholder: "Password" %><br><br>

<!--   <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%> -->

  <div><%= f.submit "Sign in", class: 'user_submit_button'  %></div>
<% end %>
<%= render "devise/shared/links" %>

</div>
</div>
