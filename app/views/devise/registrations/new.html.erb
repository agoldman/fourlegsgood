	
<div id="center-col">
<div id="signuptitle">Four Legs Good</div>

<div id="newuserbox">
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), id: "newusersignup") do |f| %>
  <%= devise_error_messages! %>


  <%= f.email_field :email, :autofocus => true, placeholder: "Email" %><br><br>


  <%= f.password_field :password, placeholder: "Password" %><br><br>


  <%= f.password_field :password_confirmation, placeholder: "Password Confirmation"  %><br><br>

  <div><%= f.submit "Sign up", class: 'user_submit_button' %></div>
<% end %>

<%= render "devise/shared/links" %>

</div>
</div>
